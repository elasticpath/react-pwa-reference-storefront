{
  "extends": ["./base-eslintrc.json"],
  "rules": {
    "react/jsx-wrap-multilines": ["off"],
    "react/destructuring-assignment": ["off"],
    "react/jsx-boolean-value": ["off"]
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": {
          "resolve": {
            "modules": ["../node_modules"],
            "extentions": [
              ".js",
              ".jsx",
              ".ts",
              ".tsx",
              ".json",
              ".png",
              ".jpg"
            ]
          }
        }
      }
    }
  }
}
