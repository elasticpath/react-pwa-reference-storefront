{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "outDir": "./dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "allowJs": true,
    "lib": [
      "es5",
      "es2015",
      "dom",
      "es6"
    ],
    "strict": false,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "isolatedModules": true
  }
}
